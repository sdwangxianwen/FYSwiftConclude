# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'FYSwift' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  #常用的第三方
pod 'SnapKit' #约束
pod 'Moya' #网络请求
pod 'Kingfisher' #图片处理
pod 'IQKeyboardManagerSwift' #键盘管理
pod 'KakaJSON' #字典转模型
pod 'JXSegmentedView' #分页指示器
pod 'HWPanModal' #仿抖音评论滑动消失的菜单

pod 'MJRefresh' #刷新
pod 'TABAnimated' #鱼骨动画
pod 'YBImageBrowser/Video' #图片浏览器
pod 'ZFPlayer' #视频播放

  # Pods for FYSwift

end
